{
  "name": "tsg-netlify-cms-start",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export && npm run build:cms",
    "cms": "npm run build && concurrently \"netlify-cms-proxy-server\" \"http-server -c-1 ./out\" \"npm run build:cms -- --watch\"",
    "build:cms": "webpack --config weback.config.cms.js",
    "build:robots-file": "TS_NODE_PROJECT=tsconfig.scripts.json ./node_modules/.bin/node-dev scripts/generate-robots-file.ts",
    "build:sitemap": "TS_NODE_PROJECT=tsconfig.scripts.json ./node_modules/.bin/node-dev scripts/generate-sitemap.ts",
    "start": "next start",
    "commitlint": "commitlint --from 847bab3",
    "lint": "prettier --check . && npm run commitlint"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@svgr/webpack": "^5.5.0",
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "netlify-cms-app": "2.15.72",
    "next": "^12.1.6",
    "node-dev": "^7.4.3",
    "react-markdown": "^6.0.2",
    "svg-react-loader": "^0.4.6",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "17.0.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "10.4.7",
    "concurrently": "^6.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.0",
    "frontmatter-markdown-loader": "^3.6.2",
    "gray-matter": "^4.0.3",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^1.6.0",
    "netlify-cms-proxy-server": "1.3.24",
    "next-optimized-images": "2.6.2",
    "path-browserify": "^1.0.1",
    "postcss": "8.4.14",
    "postcss-import": "14.1.0",
    "postcss-loader": "7.0.0",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^3.1.6",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.7.0"
  }
}
